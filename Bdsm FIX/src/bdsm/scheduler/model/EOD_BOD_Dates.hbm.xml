<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="bdsm.scheduler.model.EOD_BOD_Dates" table="EOD_BOD_DATES" lazy="false">
		<id name="id" column="ID" type="integer">
			<generator class="assigned" />
		</id>
		
		<property name="preLastDatProcess"   column="PRE_LAST_DAT_PROCESS"   type="date" />
		<property name="preNextDatProcess"   column="PRE_NEXT_DAT_PROCESS"   type="date" />
		<property name="preStatus"           column="PRE_STATUS"             type="string" />
		<property name="postLastDatProcess"  column="POST_LAST_DAT_PROCESS"  type="date" />
		<property name="postNextDatProcess"  column="POST_NEXT_DAT_PROCESS"  type="date" />
		<property name="postStatus"          column="POST_STATUS"            type="string" />
	</class>
</hibernate-mapping>
