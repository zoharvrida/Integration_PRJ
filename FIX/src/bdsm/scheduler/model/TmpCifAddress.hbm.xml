<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class lazy="false" name="bdsm.scheduler.model.TmpCifAddress" table="TMP_CIF_ADDRESS">
    <composite-id class="bdsm.scheduler.model.TmpCifAddressPK" name="compositeId">
      <key-property column="BATCHID" name="batchId" type="string"/>
      <key-property column="COD_FIELD_FP" name="codFieldFP" type="string"/>
      <key-property column="RECORDID" name="recordId" type="integer"/>
      <key-property column="COD_CUST_ID" name="cif" type="integer"/>
    </composite-id>
    <property column="ADDRESS1" name="address1" type="string"/>
    <property column="ADDRESS2" name="address2" type="string"/>
    <property column="ADDRESS3" name="address3" type="string"/>
    <property column="CITY" name="city" type="string"/>
    <property column="STATE" name="state" type="string"/>
    <property column="COUNTRY" name="country" type="string"/>
    <property column="POSTAL_CODE" name="postalCode" type="string"/>
    <property column="PHONE1MOBILE" name="phone1Mobile" type="string"/>
    <property column="PHONE2" name="phone2" type="string"/>
    <property column="DTMCREATED" name="dtmCreated" type="timestamp"/>
    <property column="IDCREATED"     name="idCreatedBy"  type="string"/>
    <property column="IDCREATEDSPV"    name="idCreatedSpv" type="string"/>
    <property column="DTMCREATEDSPV"   name="dtmCreatedSpv" type="timestamp"  />
    
  </class>
</hibernate-mapping>
