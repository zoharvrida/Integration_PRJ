<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"classpath://org/hibernate/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="bdsm.model.MasterLimitEtax" table="masterlimitetax" lazy="false">        
        <property name="idUser"        column="IDUSER"         type="string" />
        <property name="idTemplate"    column="IDTEMPLATE"    type="string" />
        <property name="sumLimit" column="SUMLIMIT" type="integer"   />
        <property name="flgMntStatus"     column="FLGMNTSTATUS"      type="string"   />
        <property name="idCreatedBy" column="IDCREATEDBY" type="string"   />
        <property name="idCreatedBySpv" column="IDCREATEDSPV" type="string"   />
        <property name="dtmCreated" column="DTMCREATED" type="date"   />
        <property name="dtmCreatedSpv" column="DTMCREATEDSPV" type="date"   />
    </class>
    
    <query name="MasterLimitEtax#getLimitUser">
		<![CDATA[
			SELECT sumLimit
			  FROM MasterLimitEtax 
                          where idUser = :pIdUser
			  
		]]>
   </query>
    
</hibernate-mapping>