<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class  name="bdsm.scheduler.model.UcmTmpSrc" table="ucm_tmp_src" lazy="false">
        <id       name="id"        column="id"        type="int" >
            <generator class="increment"/>
        </id>
        <property name="CodCustId"  column="COD_CUST_ID"  type="string"/>
        <property name="Addr1"  column="ADDR_1"  type="string"/>
        <property name="Addr2"  column="ADDR_2"  type="string"/>
        <property name="Addr3"  column="ADDR_3"  type="string"/>
        <property name="Mobile"  column="MOBILE"  type="string"/>
        <property name="Dob"  column="DOB"  type="date"/>
        <property name="Npwp"  column="NPWP"  type="string"/>
        <property name="NamPermAdrCity"  column="NAM_PERMADR_CITY"  type="string"/>
        <property name="NamPermAdrState"  column="NAM_PERMADR_STATE"  type="string"/>
        <property name="namPermAdrCntry"  column="NAM_PERMADR_CNTRY"  type="string"/>
        <property name="TxtPermAdrZip"  column="TXT_PERMADR_ZIP"  type="string"/>
        <property name="NoBatch"  column="NOBATCH"  type="string"/>
        <property name="FlgStatus"  column="FLGSTATUS"  type="string"/>
        <property name="StatusReason"  column="STATUSREASON"  type="string"/>
    </class>
</hibernate-mapping>