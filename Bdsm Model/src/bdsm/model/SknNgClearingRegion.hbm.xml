<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping default-access="field">
	<!-- Model -->
	<class name="bdsm.model.SknNgClearingRegion" table="SKNNG_CLRG_REGION">
		<id name="codSector" column="COD_SECTOR" type="integer" />
		
		<property name="codZone"      column="COD_ZONE"     type="string" />
		<property name="namSector"    column="NAM_SECTOR"   type="string" />
		<property name="idCreatedBy"  column="ID_CREATED"   type="string" />
		<property name="dtmCreated"   column="DTM_CREATED"  type="timestamp" /> 
	</class>
</hibernate-mapping>