<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"classpath://org/hibernate/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
   <class name="bdsm.model.MasterUser" table="MASTERUSER" lazy="false">
      <id       name="idUser"        column="idUser"        type="string" >
         <generator class="assigned"/>
      </id>
      <property name="idTemplate"    column="idTemplate"    type="string"/>
      <property name="namUser"       column="namUser"       type="string"/>
      <property name="cdBranch"      column="cdBranch"      type="int"   />
      <property name="isAd"          column="isAd"          type="string"/>
      <property name="isFcrMap"      column="isFcrMap"      type="string"/>
      <property name="password"      column="password"      type="string"/>
      <property name="idUserFcr"     column="idUserFcr"     type="string"/>
      <property name="idCreatedBy"   column="idCreatedBy"   type="string"/>
      <property name="idCreatedSpv"  column="idCreatedSpv"  type="string"/>
      <property name="dtmCreated"    column="dtmCreated"    type="timestamp"  />
      <property name="dtmCreatedSpv" column="dtmCreatedSpv" type="timestamp"  />
      <property name="idUpdatedBy"   column="idUpdatedBy"   type="string"/>
      <property name="idUpdatedSpv"  column="idUpdatedSpv"  type="string"/>
      <property name="dtmUpdated"    column="dtmUpdated"    type="timestamp"  />
      <property name="dtmUpdatedSpv" column="dtmUpdatedSpv" type="timestamp"  />
      <property name="datLastSignOn" column="datLastSignOn" type="timestamp"  />
      <property name="ktpUser"       column="ktpUser"       type="string"   />
      <property name="ktpPwd"        column="ktpPwd"        type="string"   />
   </class>
</hibernate-mapping>
