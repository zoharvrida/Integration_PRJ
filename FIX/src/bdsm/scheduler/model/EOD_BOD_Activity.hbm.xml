<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="bdsm.scheduler.model.EOD_BOD_Activity" table="EOD_BOD_ACTIVITY">
		<id name="activityId" column="ACTIVITY_ID" type="integer">
			<generator class="sequence">
				<param name="sequence">EOD_BOD_SEQ</param>
			</generator>
		</id>
		
		<property name="procMastId" column="PROC_MAST_ID" type="integer" />
		<property name="postingDate" column="POSTING_DATE" type="integer" />
		<property name="systemDate" column="SYSTEM_DATE" type="timestamp" />
	</class>
</hibernate-mapping>
