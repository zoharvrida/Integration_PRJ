<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"classpath://org/hibernate/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
    <class name="bdsm.model.IntErrorMapping" table="INT_ERROR_ROUTE" lazy="false">
      <composite-id>
         <key-property column="ERROR_ID" name="errNumber" type="integer"/>
         <key-property column="PROFILE_NAME" name="profileName" type="string"/>
      </composite-id>
      <property name="errCode"   column="ROUTE_ID"   type="string"/>
    </class>
</hibernate-mapping>