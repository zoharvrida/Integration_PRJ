<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="bdsm.scheduler.model.TmpGlm" table="TMP_GLM37D" lazy="false">
        <id       name="id"        column="ID"        type="integer" >
            <generator class="increment"/>
        </id>      
        <property name="batchNo"    column="BATCHNO"    type="string"  />        
        <property name="codGLAcct"  column="COD_GL_ACCT"  type="string"/>
        <property name="codCCBrn"    column="COD_CC_BRN"    type="integer"  />         
