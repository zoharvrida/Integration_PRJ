<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="bdsm.fcr.model.PmFinInstDirMast" table="PM_FIN_INST_DIR_MAST" lazy="false">
		<composite-id name="compositeId" class="bdsm.fcr.model.PmFinInstDirMastPK">
			<key-property name="codFinInstId"  column="COD_FIN_INST_ID"  type="string" />
			<key-property name="flgMntStatus"  column="FLG_MNT_STATUS"   type="string" />
			<key-property name="codEntityVPD"  column="COD_ENTITY_VPD"   type="integer" />
		</composite-id>
		
		<property name="namBank"    column="NAM_BANK"     type="string" />
		<property name="namBranch"  column="NAM_BRANCH"   type="string" />
		<property name="codZoneId"  column="COD_ZONE_ID"  type="string" />
		<property name="codBank"    column="COD_BANK"     type="string" />
		<property name="codBI"      column="COD_BI"       type="string" />
	</class>
</hibernate-mapping>